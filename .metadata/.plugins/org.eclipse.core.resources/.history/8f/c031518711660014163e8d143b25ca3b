
public class position {
	private Integer value;
	private char romanChar;
	
	public position(final Integer value){
		this.value = value;
	}
	
	public position(final char romanChar){
		this.romanChar = romanChar;
	}
	
	public Integer getValue(){
		return value;
	}
	
	public char getChar(){
		return romanChar;
	}

}
